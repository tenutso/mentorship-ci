}

model referral_payouts {
  id             Int      @id @default(autoincrement())
  user_id        Int
  payout_method  String   @db.VarChar(255)
  method_details String   @db.VarChar(255)
  amount         BigInt
  transaction_id String?  @db.VarChar(255)
  status         Int
  created_at     DateTime @db.DateTime(0)
}

model referral_settings {
  id                 Int     @id @default(autoincrement())
  is_enable          String  @db.VarChar(255)
  referral_policy    String  @db.Text
  commision_rate     String  @db.VarChar(255)
