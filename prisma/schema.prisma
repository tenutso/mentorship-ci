generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider          = "mysql"
  url               = env("DATABASE_URL")
}

model achievements {
  id            Int     @id @default(autoincrement())
  user_id       Int
  mentee_id     Int
  title         String  @db.VarChar(255)
  limit         String  @db.VarChar(255)
  short_details String  @db.Text
  image         String? @db.VarChar(255)
  thumb         String? @db.VarChar(255)
  date          String  @db.VarChar(255)
}

model assaign_days {
  id         Int     @id @default(autoincrement())
  user_id    Int
  session_id Int     @default(0)
  day        Int
  start      String? @db.VarChar(255)
  end        String? @db.VarChar(255)
}

model assign_time {
  id              Int    @id @default(autoincrement())
  user_id         Int
  session_id      Int    @default(0)
  day_id          Int
  time            String @db.VarChar(255)
  start           String @db.VarChar(255)
  end             String @db.VarChar(255)
  person_per_slot Int?
}

model blog_category {
  id      Int    @id @default(autoincrement())
  user_id Int
  name    String @db.VarChar(255)
  slug    String @db.VarChar(255)
  status  Int
}

model blog_posts {
  id          Int       @id @default(autoincrement())
  title       String?   @db.VarChar(255)
  slug        String?   @db.VarChar(255)
  type        String?   @db.VarChar(255)
  details     String?   @db.Text
  user_id     Int?
  category_id Int?
  image       String?   @db.VarChar(255)
  thumb       String?   @db.VarChar(255)
  status      Int?
  hit         Int?
  is_featured Int?
  created_at  DateTime? @db.DateTime(0)
}

model blogs {
  id          Int      @id @default(autoincrement())
  lang_id     String?  @db.VarChar(155)
  business_id String?  @db.VarChar(155)
  user_id     Int
  category_id Int
  title       String   @db.VarChar(255)
  slug        String   @db.VarChar(255)
  details     String?  @db.LongText
  image       String?  @db.VarChar(255)
  thumb       String?  @db.VarChar(255)
  tags        String?  @db.VarChar(255)
  meta_tags   String?  @db.VarChar(255)
  meta_desc   String?  @db.VarChar(255)
  status      Int
  total_views Int?
  created_at  DateTime @db.DateTime(0)
}

model brands {
  id      Int     @id @default(autoincrement())
  user_id Int
  name    String? @db.VarChar(255)
  logo    String  @db.VarChar(255)
  link    String  @db.VarChar(255)
  status  Int
}

model business {
  id                 Int     @id @default(autoincrement())
  uid                String  @db.VarChar(255)
  user_id            Int
  name               String? @db.VarChar(255)
  slug               String? @db.VarChar(255)
  details            String? @db.Text
  keywords           String? @db.Text
  description        String? @db.Text
  type               Int     @default(1)
  title              String? @db.VarChar(255)
  country            Int?
  address            String? @db.MediumText
  email              String? @db.VarChar(255)
  phone              String? @db.VarChar(255)
  category           String? @db.VarChar(255)
  business_type      Int?
  logo               String? @db.VarChar(255)
  image              String? @db.Text
  thumb              String? @db.Text
  breadcrumb_banner  String? @db.VarChar(255)
  hero_img           String? @db.VarChar(255)
  status             Int     @default(0)
  enable_gallery     String? @default("1") @db.VarChar(255)
  enable_staff       String  @default("1") @db.VarChar(255)
  is_primary         Int?
  template_style     Int     @default(1)
  curr_locate        String? @default("0") @db.VarChar(155)
  num_format         String? @default("0") @db.VarChar(155)
  color              String  @default("#546af1") @db.VarChar(255)
  facebook           String? @db.VarChar(255)
  twitter            String? @db.VarChar(255)
  instagram          String? @db.VarChar(255)
  whatsapp           String? @db.VarChar(255)
  qr_code            String? @db.VarChar(255)
  time_zone          Int?    @default(1)
  date_format        String? @default("d M Y") @db.VarChar(255)
  time_format        String? @default("hh") @db.VarChar(255)
  time_interval      String? @default("30") @db.VarChar(255)
  interval_type      String? @default("minute") @db.VarChar(255)
  interval_settings  String? @default("2") @db.VarChar(155)
  enable_category    String? @default("0") @db.VarChar(155)
  enable_rating      String? @default("0") @db.VarChar(155)
  enable_location    String? @default("0") @db.VarChar(155)
  enable_group       String? @default("0") @db.VarChar(155)
  enable_guest       String? @default("0") @db.VarChar(155)
  total_person       String? @default("5") @db.VarChar(155)
  enable_payment     String? @default("1") @db.VarChar(155)
  enable_onsite      String? @default("1") @db.VarChar(155)
  enable_blog        String? @db.VarChar(155)
  enable_portfolio   String? @db.VarChar(155)
  enable_product     String? @db.VarChar(155)
  enable_team        String? @db.VarChar(155)
  enable_counter     String? @db.VarChar(155)
  enable_career      String? @db.VarChar(155)
  enable_service     String? @db.VarChar(155)
  enable_event       String? @db.VarChar(155)
  enable_quote       String? @db.VarChar(155)
  enable_faq         String? @db.VarChar(155)
  enable_testimonial String? @db.VarChar(155)
  enable_appointment String? @db.VarChar(155)
  holidays           String? @db.LongText
  about_title        String? @db.VarChar(255)
  about_details      String? @db.Text
  about_vedio_url    String? @db.VarChar(255)
  home_style         String? @db.VarChar(155)
  pagination_limit   String? @db.VarChar(255)
  created_at         String? @db.VarChar(255)
}

model categories {
  id        Int     @id @default(autoincrement())
  icon      String? @db.VarChar(255)
  name      String  @db.VarChar(255)
  slug      String  @db.VarChar(255)
  details   String? @db.Text
  direction String? @db.Text
  status    Int
}

model collections {
  id         Int    @id @default(autoincrement())
  user_id    Int
  name       String @db.VarChar(255)
  slug       String @db.VarChar(255)
  status     Int
  created_at String @db.VarChar(255)
}

model comments {
  id         Int      @id @default(autoincrement())
  user_id    Int?
  post_id    Int?
  name       String?  @db.VarChar(255)
  email      String?  @db.VarChar(255)
  message    String?  @db.Text
  created_at DateTime @db.DateTime(0)
}

model contacts {
  id          Int      @id @default(autoincrement())
  business_id String?  @db.VarChar(255)
  name        String?  @db.VarChar(255)
  email       String?  @db.VarChar(255)
  message     String?  @db.Text
  created_at  DateTime @db.DateTime(0)
}

model counters {
  id          Int     @id @default(autoincrement())
  lang_id     String? @db.VarChar(155)
  business_id String? @db.VarChar(155)
  user_id     Int
  icon        String  @db.VarChar(255)
  number      String  @db.VarChar(255)
  title       String  @db.VarChar(255)
  text        String  @db.Text
  status      Int
}

model country {
  id              Int    @id @default(autoincrement())
  name            String @db.VarChar(20)
  code            String @unique(map: "code") @db.VarChar(2)
  dial_code       String @db.VarChar(5)
  currency_name   String @db.VarChar(20)
  currency_symbol String @db.VarChar(20)
  currency_code   String @db.VarChar(10)
}

model coupon_apply {
  id         Int      @id @default(autoincrement())
  coupon_id  Int
  user_id    String   @db.VarChar(255)
  mentee_id  Int
  session_id Int
  code       String   @db.VarChar(255)
  discount   Int
  created_at DateTime @db.Date
}

model coupons {
  id              Int      @id @default(autoincrement())
  user_id         Int
  session_id      Int
  code            String   @db.VarChar(255)
  discount        String   @db.VarChar(255)
  start_date      String?  @db.VarChar(255)
  end_date        String?  @db.VarChar(255)
  once_per_mentee Int?     @default(0)
  usages_limit    String?  @db.VarChar(255)
  used            Int?     @default(0)
  status          Int?     @default(1)
  created_at      DateTime @db.DateTime(0)
}

model dialing_codes {
  id       Int     @id @default(autoincrement())
  name     String  @default("") @db.VarChar(64)
  iso_code String  @db.VarChar(2)
  isd_code String? @db.VarChar(7)
}

model educations {
  id         Int     @id @default(autoincrement())
  user_id    String  @db.VarChar(255)
  institute  String  @db.VarChar(255)
  degree     String  @db.VarChar(255)
  start_year String  @db.VarChar(255)
  end_year   String? @db.VarChar(255)
  status     String  @db.VarChar(255)
  created_at String? @db.VarChar(255)
}

model experiences {
  id           Int     @id @default(autoincrement())
  user_id      Int
  icon         String  @db.VarChar(255)
  title        String  @db.VarChar(255)
  company      String? @db.VarChar(255)
  start_date   String? @db.VarChar(255)
  end_date     String? @db.VarChar(255)
  is_present   String  @default("0") @db.VarChar(255)
  contribution String? @db.Text
  status       String  @db.VarChar(255)
  created_at   String? @db.VarChar(255)
}

model faqs {
  id          Int     @id @default(autoincrement())
  lang_id     String  @default("1") @db.VarChar(155)
  business_id String? @db.VarChar(255)
  type        String  @default("admin") @db.VarChar(155)
  title       String? @db.VarChar(255)
  details     String? @db.MediumText
}

model favourite {
  id           Int    @id @default(autoincrement())
  user_id      Int
  favourite_id Int
  created_at   String @db.VarChar(255)
}

model feature_assaign {
  id         Int @id @default(autoincrement())
  package_id Int
  feature_id Int
}

model features {
  id        Int     @id @default(autoincrement())
  name      String? @db.VarChar(255)
  slug      String  @db.VarChar(255)
  is_limit  Int
  basic     Int?
  standared Int?
  premium   Int?
}

model fonts {
  id      Int    @id @default(autoincrement())
  user_id Int
  name    String @db.VarChar(255)
  slug    String @db.VarChar(255)
}

model kyc_verifications {
  id               Int     @id @default(autoincrement())
  country_id       Int
  user_id          Int
  document_type    String  @db.VarChar(255)
  doc_id_number    String  @db.VarChar(255)
  first_name       String  @db.VarChar(255)
  last_name        String? @db.VarChar(255)
  birth_date       String  @db.VarChar(255)
  address          String? @db.Text
  front_side_doc   String? @db.VarChar(255)
  back_side_doc    String? @db.VarChar(255)
  selfiee_with_doc String? @db.VarChar(255)
  passport         String? @db.VarChar(255)
  status           String? @default("0") @db.VarChar(255)
  reject_reason    String? @db.Text
  is_preview       Int?    @default(0)
  created_at       String? @db.VarChar(255)
  resub_date       String? @db.VarChar(255)
}

model lang_values {
  id      Int     @id @default(autoincrement())
  type    String? @db.VarChar(255)
  label   String  @db.VarChar(255)
  keyword String  @db.VarChar(255)
  english String? @db.Text
}

model language {
  id             Int     @id @default(autoincrement())
  name           String  @db.VarChar(255)
  slug           String  @db.VarChar(255)
  short_name     String  @db.VarChar(255)
  code           String? @db.VarChar(255)
  text_direction String  @db.VarChar(255)
  status         Int
}

model messages {
  id            Int      @id @default(autoincrement())
  mgs_from      Int?
  mgs_to        Int?
  message       String?  @db.Text
  mgs_time      DateTime @db.DateTime(0)
  mgs_seen      Boolean? @default(false)
  mgs_seen_time String?  @db.VarChar(255)
  ongoing_id    Int?
  reply_id      Int?
}

model notifications {
  id         Int      @id @default(autoincrement())
  user_id    Int?
  action_id  Int?
  content_id Int?
  text       String?  @db.VarChar(255)
  noti_type  Int?
  noti_time  DateTime @db.DateTime(0)
  seen       Int      @default(0)
}

/// This model has been renamed to 'Renamedpackage' during introspection, because the original name 'package' is reserved.
model Renamedpackage {
  id             Int      @id @default(autoincrement())
  name           String?  @db.VarChar(255)
  slug           String?  @db.VarChar(255)
  price          Decimal? @default(0.00) @db.Decimal(10, 2)
  monthly_price  Decimal? @db.Decimal(10, 2)
  lifetime_price Decimal  @default(1000.00) @db.Decimal(10, 2)
  bill_type      String?  @db.VarChar(255)
  is_special     Int      @default(0)
  status         Int      @default(1)

  @@map("package")
}

model pages {
  id          Int     @id @default(autoincrement())
  lang_id     String  @default("1") @db.VarChar(155)
  business_id String? @db.VarChar(255)
  type        String? @default("admin") @db.VarChar(155)
  title       String? @db.VarChar(255)
  slug        String? @db.VarChar(255)
  details     String? @db.LongText
  status      Int?
  created_at  String? @db.VarChar(255)
}

model payment {
  id             Int       @id @default(autoincrement())
  puid           String    @db.VarChar(255)
  user_id        Int
  package_id     String?   @db.VarChar(255)
  billing_type   String?   @db.VarChar(255)
  amount         Decimal?  @db.Decimal(10, 2)
  status         String    @db.VarChar(255)
  created_at     DateTime  @db.Date
  expire_on      DateTime? @db.Date
  payment_method String?   @db.VarChar(255)
  proof          String?   @db.Text
  tax            String?   @default("0") @db.VarChar(255)
}

model payment_user {
  id                Int      @id @default(autoincrement())
  puid              String   @db.VarChar(255)
  user_id           Int
  customer_id       Int
  booking_id        Int
  amount            Decimal? @db.Decimal(10, 2)
  total_amount      Decimal? @default(0.00) @db.Decimal(10, 2)
  commission_amount Decimal? @default(0.00) @db.Decimal(10, 2)
  commission_rate   Int?     @default(0)
  status            String   @db.VarChar(255)
  created_at        DateTime @db.Date
  payment_method    String?  @db.VarChar(255)
  proof             String?  @db.VarChar(255)
  type              String   @default("user") @db.VarChar(155)
}

model payouts {
  id             Int      @id @default(autoincrement())
  user_id        Int
  payout_method  String   @db.VarChar(255)
  amount         BigInt
  transaction_id String?  @db.VarChar(255)
  message        String?  @db.Text
  currency       String?  @db.VarChar(255)
  status         Int
  created_at     DateTime @db.DateTime(0)
}

model plan_coupons {
  id            Int       @id @default(autoincrement())
  uid           String    @db.VarChar(255)
  name          String?   @db.VarChar(255)
  user_id       String?   @default("0") @db.VarChar(155)
  plan          String    @db.VarChar(255)
